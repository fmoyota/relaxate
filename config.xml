<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "ec.com.edifarm.difarerelaxate"
    versionCode = "10"
    version     = "1.0.1" >

<!-- versionCode is optional and Android only -->

  <name>RELAXATE</name>

  <description>
      Falvoxato Clorhidrato
  </description>

  <author href="https://www.edifarm.com.ec" email="fmoyota@edifarm.com.ec">
      Fernando Moyota - Edifarm
  </author>
    
<platform name="android" />
	<icon src="images/logo.png" />
<platform name="android">
	<icon qualifier="ldpi" src="images/android/drawable-ldpi/icon.png" />
	<icon qualifier="mdpi" src="images/android/drawable-mdpi/icon.png" />
	<icon qualifier="hdpi" src="images/android/drawable-hdpi/icon.png" />
	<icon qualifier="xhdpi" src="images/android/drawable-xhdpi/icon.png" />
	<icon qualifier="xxhdpi" src="images/android/drawable-xxhdpi/icon.png" />
	<icon qualifier="xxxhdpi" src="images/android/drawable-xxxhdpi/icon.png" />
	<splash qualifier="land-ldpi" src="images/android/drawable-land-ldpi/screen.png" />
	<splash qualifier="land-mdpi" src="images/android/drawable-land-mdpi/screen.png" />
	<splash qualifier="land-hdpi" src="images/android/drawable-land-hdpi/screen.png" />
	<splash qualifier="land-xhdpi" src="images/android/drawable-land-xhdpi/screen.png" />
	<splash qualifier="land-xxhdpi" src="images/android/drawable-land-xxhdpi/screen.png" />
	<splash qualifier="land-xxxhdpi" src="images/android/drawable-land-xxxhdpi/screen.png" />
	<splash qualifier="port-ldpi" src="images/android/drawable-ldpi/screen.png" />
	<splash qualifier="port-mdpi" src="images/android/drawable-mdpi/screen.png" />
	<splash qualifier="port-hdpi" src="images/android/drawable-hdpi/screen.png" />
	<splash qualifier="port-xhdpi" src="images/android/drawable-xhdpi/screen.png" />
	<splash qualifier="port-xxhdpi" src="images/android/drawable-xxhdpi/screen.png" />
	<splash qualifier="port-xxxhdpi" src="images/android/drawable-xxxhdpi/screen.png" />
</platform>

	
	
	
	
	
  <!-- all platforms -->
  <preference name="phonegap-version" value="cli-8.0.0" />
  <preference name="orientation" value="landscape" />
  <preference name="fullscreen" value="true" />

  <!-- iOS only -->
  <preference name="target-device" value="universal" />
  <preference name="prerendered-icon" value="true" />
  <preference name="detect-data-types" value="true" />
  <preference name="exit-on-suspend" value="true" />
  <preference name="deployment-target" value="7.0" />

  <!-- Android only -->
  <preference name="android-build-tool" value="ant|gradle" />
  <preference name="android-minSdkVersion" value="16" />
  <preference name="android-maxSdkVersion" value="26" />
  <preference name="android-targetSdkVersion" value="16" />
  <preference name="android-installLocation" value="auto" />
  <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />

	
	
	
	<plugin name="cordova-plugin-whitelist" />
	
	
</widget>